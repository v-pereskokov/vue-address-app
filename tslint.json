{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "array-type": [
      true,
      "generic"
    ],
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "any"
      }
    ],
    "object-literal-sort-keys": false,
    "variable-name": false,
    "no-console": false,
    "max-line-length": [
      true,
      200
    ]
  },
  "rulesDirectory": []
}
